
@{
    ViewBag.Title = "Migracao de Mapas";
}

@using (Html.BeginForm())
{
    <div class="container">
        <h5 class="amber-text text-darken-4">Migracao de Mapas</h5>
        <div class="row">
            <div class="input-field col s12 m6 l6">
                <strong>Mapa</strong>
                @Html.DropDownList("ListaMapa", null, "Selecione")
            </div>
            <div class="input-field col s12 m6 l6">
                <strong>Mapa de Migração</strong>
                @Html.DropDownList("ListaMapaMigracao", null, "Selecione")
            </div>
        </div>

        <input type="submit" value="Migrar" class="waves-effect waves-light btn" />

    </div>
}

@section Scriptsoutros
{
    <script type="text/javascript">
        $(document).ready(function () {
            $('select').material_select();
        });
    </script>
}
